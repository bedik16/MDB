<Administration>:
    canvas.before:
        Rectangle:
            pos: self.pos
            size: self.size
            source: "background.jpg"

    GridLayout:
        cols: 1
        padding: [50, 10, 50, 10]
        BoxLayout:
            size_hint_y: .2
            orientation: "vertical"
            Label:
                text: "Administration Settings"
                font_size: 32

        BoxLayout:
            orientation: "vertical"
            size_hint_y: .1
            Button:
                text: "Cancel"
                font_size: 24
                on_press: root.do_cancel()

